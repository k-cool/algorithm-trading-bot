{
  "extends": ["plugin:prettier/recommended", "eslint:recommended", "airbnb"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ],
    "no-restricted-syntax": "off",
    "no-unused-vars": "warn",
    "no-prototype-builtins": "off",
    "quotes": "off",
    "linebreak-style": "off",
    "comma-dangle": "off",
    "no-trailing-spaces": "off",
    "dot-notation": "warn",
    "no-plusplus": "off",
    "spaced-comment": "off",
    "no-unused-expressions": "off",
    "class-methods-use-this": "off",
    "wrap-iife": "off",
    "curly": "off",
    "nonblock-statement-body-position": "off",
    "operator-linebreak": "off",
    "no-continue": "off",
    "no-alert": "off",
    "eqeqeq": "off",
    "indent": "off",
    "import/prefer-default-export": "off",
    "object-curly-newline": "off"
  }
}
